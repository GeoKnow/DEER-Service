    <servlet-name>GeoLiftRun</servlet-name>
    <servlet-class>org.linkeddata.stack.service.geolift.GeoLiftRun</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GeoLiftReview</servlet-name>
    <servlet-class>org.linkeddata.stack.service.geolift.GeoLiftReview</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoadFile</servlet-name>
    <servlet-class>org.linkeddata.stack.service.geolift.LoadFile</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ImportRDF</servlet-name>
    <servlet-class>org.linkeddata.stack.service.geolift.ImportRDF</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GeoLiftRun</servlet-name>
    <url-pattern>/GeoLiftRun</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GeoLiftReview</servlet-name>
    <url-pattern>/GeoLiftReview</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoadFile</servlet-name>
    <url-pattern>/LoadFile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ImportRDF</servlet-name>
    <url-pattern>/ImportRDF</url-pattern>
  </servlet-mapping>
</web-app>